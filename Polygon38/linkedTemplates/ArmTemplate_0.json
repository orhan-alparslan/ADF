{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "Polygon38"
		},
		"ls_adls2_ak_01_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'ls_adls2_ak_01'"
		},
		"ls_sqldb_ak_01_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'ls_sqldb_ak_01'"
		},
		"Polygon_keyvault_LS_properties_typeProperties_baseUrl": {
			"type": "string",
			"defaultValue": "https://keyssigmabi.vault.azure.net/"
		},
		"ls_adls2_ak_01_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://polygon38.dfs.core.windows.net/"
		},
		"Polygon_LS_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://api.polygon.io/"
		},
		"ls_ats_ak_01_properties_typeProperties_connectionString_secretName": {
			"type": "string",
			"defaultValue": "blobstorage-polygon38-connection-string"
		},
		"ls_polygon_stocks_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://api.polygon.io/"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "[concat(parameters('factoryName'), '/Polygon_keyvault_LS')]",
			"type": "Microsoft.DataFactory/factories/linkedServices",
			"apiVersion": "2018-06-01",
			"properties": {
				"annotations": [],
				"type": "AzureKeyVault",
				"typeProperties": {
					"baseUrl": "[parameters('Polygon_keyvault_LS_properties_typeProperties_baseUrl')]"
				}
			},
			"dependsOn": []
		},
		{
			"name": "[concat(parameters('factoryName'), '/ls_adls2_ak_01')]",
			"type": "Microsoft.DataFactory/factories/linkedServices",
			"apiVersion": "2018-06-01",
			"properties": {
				"annotations": [],
				"type": "AzureBlobFS",
				"typeProperties": {
					"url": "[parameters('ls_adls2_ak_01_properties_typeProperties_url')]",
					"accountKey": {
						"type": "SecureString",
						"value": "[parameters('ls_adls2_ak_01_accountKey')]"
					}
				}
			},
			"dependsOn": []
		},
		{
			"name": "[concat(parameters('factoryName'), '/default')]",
			"type": "Microsoft.DataFactory/factories/managedVirtualNetworks",
			"apiVersion": "2018-06-01",
			"properties": {},
			"dependsOn": []
		},
		{
			"name": "[concat(parameters('factoryName'), '/ds_adls2_parq_stocks')]",
			"type": "Microsoft.DataFactory/factories/datasets",
			"apiVersion": "2018-06-01",
			"properties": {
				"linkedServiceName": {
					"referenceName": "ls_adls2_ak_01",
					"type": "LinkedServiceReference"
				},
				"parameters": {
					"ds_blobName": {
						"type": "string"
					},
					"ds_fileName": {
						"type": "string"
					},
					"ds_directoryName": {
						"type": "string"
					}
				},
				"folder": {
					"name": "output"
				},
				"annotations": [],
				"type": "Parquet",
				"typeProperties": {
					"location": {
						"type": "AzureBlobFSLocation",
						"fileName": {
							"value": "@dataset().ds_fileName",
							"type": "Expression"
						},
						"folderPath": {
							"value": "@dataset().ds_directoryName",
							"type": "Expression"
						},
						"fileSystem": {
							"value": "@dataset().ds_blobName",
							"type": "Expression"
						}
					},
					"compressionCodec": "snappy"
				},
				"schema": []
			},
			"dependsOn": [
				"[concat(variables('factoryId'), '/linkedServices/ls_adls2_ak_01')]"
			]
		},
		{
			"name": "[concat(parameters('factoryName'), '/Polygon_LS')]",
			"type": "Microsoft.DataFactory/factories/linkedServices",
			"apiVersion": "2018-06-01",
			"properties": {
				"annotations": [],
				"type": "RestService",
				"typeProperties": {
					"url": "[parameters('Polygon_LS_properties_typeProperties_url')]",
					"enableServerCertificateValidation": true,
					"authenticationType": "Anonymous",
					"authHeaders": {
						"Authorization": {
							"type": "AzureKeyVaultSecret",
							"store": {
								"referenceName": "Polygon_keyvault_LS",
								"type": "LinkedServiceReference"
							},
							"secretName": "Polygon-auth-header-free"
						}
					}
				}
			},
			"dependsOn": [
				"[concat(variables('factoryId'), '/linkedServices/Polygon_keyvault_LS')]"
			]
		},
		{
			"name": "[concat(parameters('factoryName'), '/ls_ats_ak_01')]",
			"type": "Microsoft.DataFactory/factories/linkedServices",
			"apiVersion": "2018-06-01",
			"properties": {
				"annotations": [],
				"type": "AzureTableStorage",
				"typeProperties": {
					"connectionString": {
						"type": "AzureKeyVaultSecret",
						"store": {
							"referenceName": "Polygon_keyvault_LS",
							"type": "LinkedServiceReference"
						},
						"secretName": "[parameters('ls_ats_ak_01_properties_typeProperties_connectionString_secretName')]"
					}
				}
			},
			"dependsOn": [
				"[concat(variables('factoryId'), '/linkedServices/Polygon_keyvault_LS')]"
			]
		},
		{
			"name": "[concat(parameters('factoryName'), '/integrationRuntime-interactive')]",
			"type": "Microsoft.DataFactory/factories/integrationRuntimes",
			"apiVersion": "2018-06-01",
			"properties": {
				"type": "Managed",
				"typeProperties": {
					"computeProperties": {
						"location": "AutoResolve",
						"dataFlowProperties": {
							"computeType": "General",
							"coreCount": 8,
							"timeToLive": 10,
							"cleanup": false
						},
						"pipelineExternalComputeScaleProperties": {
							"timeToLive": 60
						}
					}
				},
				"managedVirtualNetwork": {
					"type": "ManagedVirtualNetworkReference",
					"referenceName": "default"
				}
			},
			"dependsOn": [
				"[concat(variables('factoryId'), '/managedVirtualNetworks/default')]"
			]
		},
		{
			"name": "[concat(parameters('factoryName'), '/ls_polygon_stocks')]",
			"type": "Microsoft.DataFactory/factories/linkedServices",
			"apiVersion": "2018-06-01",
			"properties": {
				"annotations": [],
				"type": "RestService",
				"typeProperties": {
					"url": "[parameters('ls_polygon_stocks_properties_typeProperties_url')]",
					"enableServerCertificateValidation": true,
					"authenticationType": "Anonymous",
					"authHeaders": {
						"Authorization": {
							"type": "AzureKeyVaultSecret",
							"store": {
								"referenceName": "Polygon_keyvault_LS",
								"type": "LinkedServiceReference"
							},
							"secretName": "Polygon-auth-header-stocks"
						}
					}
				},
				"connectVia": {
					"referenceName": "integrationRuntime-interactive",
					"type": "IntegrationRuntimeReference"
				}
			},
			"dependsOn": [
				"[concat(variables('factoryId'), '/integrationRuntimes/integrationRuntime-interactive')]",
				"[concat(variables('factoryId'), '/linkedServices/Polygon_keyvault_LS')]"
			]
		},
		{
			"name": "[concat(parameters('factoryName'), '/ls_sqldb_ak_01')]",
			"type": "Microsoft.DataFactory/factories/linkedServices",
			"apiVersion": "2018-06-01",
			"properties": {
				"annotations": [],
				"type": "AzureSqlDatabase",
				"typeProperties": {
					"connectionString": "[parameters('ls_sqldb_ak_01_connectionString')]"
				},
				"connectVia": {
					"referenceName": "integrationRuntime-interactive",
					"type": "IntegrationRuntimeReference"
				}
			},
			"dependsOn": [
				"[concat(variables('factoryId'), '/integrationRuntimes/integrationRuntime-interactive')]"
			]
		},
		{
			"name": "[concat(parameters('factoryName'), '/R_Conditions')]",
			"type": "Microsoft.DataFactory/factories/datasets",
			"apiVersion": "2018-06-01",
			"properties": {
				"linkedServiceName": {
					"referenceName": "ls_polygon_stocks",
					"type": "LinkedServiceReference"
				},
				"folder": {
					"name": "input/rest"
				},
				"annotations": [],
				"type": "RestResource",
				"typeProperties": {
					"relativeUrl": "v3/reference/conditions?asset_class=stocks&apiKey="
				},
				"schema": []
			},
			"dependsOn": [
				"[concat(variables('factoryId'), '/linkedServices/ls_polygon_stocks')]"
			]
		},
		{
			"name": "[concat(parameters('factoryName'), '/R_Exchanges')]",
			"type": "Microsoft.DataFactory/factories/datasets",
			"apiVersion": "2018-06-01",
			"properties": {
				"linkedServiceName": {
					"referenceName": "ls_polygon_stocks",
					"type": "LinkedServiceReference"
				},
				"folder": {
					"name": "input/rest"
				},
				"annotations": [],
				"type": "RestResource",
				"typeProperties": {
					"relativeUrl": "v3/reference/exchanges"
				},
				"schema": []
			},
			"dependsOn": [
				"[concat(variables('factoryId'), '/linkedServices/ls_polygon_stocks')]"
			]
		},
		{
			"name": "[concat(parameters('factoryName'), '/R_MarketHolidays')]",
			"type": "Microsoft.DataFactory/factories/datasets",
			"apiVersion": "2018-06-01",
			"properties": {
				"linkedServiceName": {
					"referenceName": "ls_polygon_stocks",
					"type": "LinkedServiceReference"
				},
				"folder": {
					"name": "input/rest"
				},
				"annotations": [],
				"type": "RestResource",
				"typeProperties": {
					"relativeUrl": "v1/marketstatus/upcoming?apiKey="
				},
				"schema": []
			},
			"dependsOn": [
				"[concat(variables('factoryId'), '/linkedServices/ls_polygon_stocks')]"
			]
		},
		{
			"name": "[concat(parameters('factoryName'), '/R_MarketHolidays_ls')]",
			"type": "Microsoft.DataFactory/factories/datasets",
			"apiVersion": "2018-06-01",
			"properties": {
				"linkedServiceName": {
					"referenceName": "ls_polygon_stocks",
					"type": "LinkedServiceReference"
				},
				"folder": {
					"name": "input/rest"
				},
				"annotations": [],
				"type": "RestResource",
				"typeProperties": {
					"relativeUrl": "v1/marketstatus/upcoming?apiKey="
				},
				"schema": []
			},
			"dependsOn": [
				"[concat(variables('factoryId'), '/linkedServices/ls_polygon_stocks')]"
			]
		},
		{
			"name": "[concat(parameters('factoryName'), '/R_MarketStatus')]",
			"type": "Microsoft.DataFactory/factories/datasets",
			"apiVersion": "2018-06-01",
			"properties": {
				"linkedServiceName": {
					"referenceName": "ls_polygon_stocks",
					"type": "LinkedServiceReference"
				},
				"folder": {
					"name": "input/rest"
				},
				"annotations": [],
				"type": "RestResource",
				"typeProperties": {
					"relativeUrl": "v1/marketstatus/now?apiKey="
				},
				"schema": []
			},
			"dependsOn": [
				"[concat(variables('factoryId'), '/linkedServices/ls_polygon_stocks')]"
			]
		},
		{
			"name": "[concat(parameters('factoryName'), '/R_TickerNews')]",
			"type": "Microsoft.DataFactory/factories/datasets",
			"apiVersion": "2018-06-01",
			"properties": {
				"linkedServiceName": {
					"referenceName": "ls_polygon_stocks",
					"type": "LinkedServiceReference"
				},
				"folder": {
					"name": "input/rest"
				},
				"annotations": [],
				"type": "RestResource",
				"typeProperties": {
					"relativeUrl": "https://api.polygon.io/v2/reference/news?order=desc&sort=published_utc&apiKey="
				},
				"schema": []
			},
			"dependsOn": [
				"[concat(variables('factoryId'), '/linkedServices/ls_polygon_stocks')]"
			]
		},
		{
			"name": "[concat(parameters('factoryName'), '/R_TickerTypes')]",
			"type": "Microsoft.DataFactory/factories/datasets",
			"apiVersion": "2018-06-01",
			"properties": {
				"linkedServiceName": {
					"referenceName": "ls_polygon_stocks",
					"type": "LinkedServiceReference"
				},
				"folder": {
					"name": "input/rest"
				},
				"annotations": [],
				"type": "RestResource",
				"typeProperties": {
					"relativeUrl": "v3/reference/tickers/types"
				},
				"schema": []
			},
			"dependsOn": [
				"[concat(variables('factoryId'), '/linkedServices/ls_polygon_stocks')]"
			]
		},
		{
			"name": "[concat(parameters('factoryName'), '/R_Tickers')]",
			"type": "Microsoft.DataFactory/factories/datasets",
			"apiVersion": "2018-06-01",
			"properties": {
				"linkedServiceName": {
					"referenceName": "ls_polygon_stocks",
					"type": "LinkedServiceReference"
				},
				"folder": {
					"name": "input/rest"
				},
				"annotations": [],
				"type": "RestResource",
				"typeProperties": {
					"relativeUrl": "v3/reference/tickers?sort=ticker"
				},
				"schema": []
			},
			"dependsOn": [
				"[concat(variables('factoryId'), '/linkedServices/ls_polygon_stocks')]"
			]
		},
		{
			"name": "[concat(parameters('factoryName'), '/R_fact_stocks_aggregates')]",
			"type": "Microsoft.DataFactory/factories/datasets",
			"apiVersion": "2018-06-01",
			"properties": {
				"linkedServiceName": {
					"referenceName": "ls_polygon_stocks",
					"type": "LinkedServiceReference"
				},
				"folder": {
					"name": "input/rest"
				},
				"annotations": [],
				"type": "RestResource",
				"typeProperties": {
					"relativeUrl": "v2/aggs/ticker/AAPL/range/1/day/2023-01-09/2023-01-09?adjusted=true&sort=asc&limit=120"
				},
				"schema": []
			},
			"dependsOn": [
				"[concat(variables('factoryId'), '/linkedServices/ls_polygon_stocks')]"
			]
		},
		{
			"name": "[concat(parameters('factoryName'), '/R_fact_stocks_grouped_daily_bars')]",
			"type": "Microsoft.DataFactory/factories/datasets",
			"apiVersion": "2018-06-01",
			"properties": {
				"linkedServiceName": {
					"referenceName": "ls_polygon_stocks",
					"type": "LinkedServiceReference"
				},
				"folder": {
					"name": "input/rest"
				},
				"annotations": [],
				"type": "RestResource",
				"typeProperties": {
					"relativeUrl": "v2/aggs/grouped/locale/us/market/stocks/2023-01-09?adjusted=true"
				},
				"schema": []
			},
			"dependsOn": [
				"[concat(variables('factoryId'), '/linkedServices/ls_polygon_stocks')]"
			]
		},
		{
			"name": "[concat(parameters('factoryName'), '/ds_sqldb')]",
			"type": "Microsoft.DataFactory/factories/datasets",
			"apiVersion": "2018-06-01",
			"properties": {
				"linkedServiceName": {
					"referenceName": "ls_sqldb_ak_01",
					"type": "LinkedServiceReference"
				},
				"folder": {
					"name": "input/sql"
				},
				"annotations": [],
				"type": "AzureSqlTable",
				"schema": [],
				"typeProperties": {
					"schema": "dbo"
				}
			},
			"dependsOn": [
				"[concat(variables('factoryId'), '/linkedServices/ls_sqldb_ak_01')]"
			]
		}
	]
}